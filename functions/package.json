  "engines": {
    "node": "10"
  },
  "dependencies": {
    "firebase": "^7.15.5",
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.8.0",
    "node": "^14.4.0"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
